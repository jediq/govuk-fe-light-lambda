{% macro element(elementData) %}


<form method="POST">


{% from "components/fieldset/macro.njk" import fieldset %}
{% call fieldset({
  legend: {
    text: heading,
    isPageHeading: true
  }
}) %}
  

{% for eachElement in elementData.elements %}
{% from "elements/" + eachElement.type + ".njk" import element %}
{{ element(eachElement) }}
{% endfor %}




{% endcall %}

</form>
  
{% endmacro %}


