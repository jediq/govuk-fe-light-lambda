{
  "name": "govuk-fe-light",
  "version": "0.1.0",
  "description": "An accellarated light weight front end renderer for gov.uk projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Ricky Walker",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-ts && npm run lint && npm run copy-node-assets && npm run copy-public-assets",
    "serve": "node dist/framework/server.js",
    "watch-node": "nodemon dist/framework/server.js",
    "watch": "concurrently -k --handle-input npm:watch-node npm:watch-ts npm:watch-copy-node-assets npm:watch-copy-public-assets npm:watch-sass",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-node-assets": "ts-node copyNodeAssets.ts",
    "copy-public-assets": "ts-node copyPublicAssets.ts",
    "watch-copy-node-assets": "onchange 'node-modules' -- npm run copy-node-assets",
    "watch-copy-public-assets": "onchange 'src/framework/**/*.njk' -- npm run copy-public-assets",
    "debug": "npm run build && npm run watch-debug",
    "debug-serve": "nodemon --inspect dist/framework/server.js",
    "debug-watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "acorn": "^6.4.1",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "crypto-js": "^3.1.9-1",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "got": "^9.6.0",
    "govuk-frontend": "^3.0.0",
    "handlebars": "^4.1.2",
    "lodash": "^4.17.15",
    "nhsuk-frontend": "^2.3.0",
    "nunjucks": "^3.2.0",
    "onchange": "^6.1.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/aws-serverless-express": "^3.3.1",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/connect-mongo": "^0.0.42",
    "@types/cookie-parser": "^1.4.1",
    "@types/crypto-js": "^3.1.43",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/express-flash": "0.0.0",
    "@types/express-session": "^1.15.13",
    "@types/got": "^9.6.5",
    "@types/jest": "^24.0.15",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.134",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lusca": "^1.6.0",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.10",
    "@types/nodemailer": "^6.2.0",
    "@types/nunjucks": "^3.1.1",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.7",
    "@types/winston": "^2.3.9",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.0.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}