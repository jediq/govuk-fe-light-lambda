{% macro element(elementData) %}

<fieldset>

<label for="{{elementData.name}}">{{elementData.displayText}}</label>
Day:<input type="text" name="{{elementData.name}}-day">
Month:<input type="text" name="{{elementData.name}}-month">
Year:<input type="text" name="{{elementData.name}}-year">


{% if elementData.invalid %}
  error : {{ elementData.validation.error }}
{% endif %}
  
</fieldset>
{% endmacro %}


