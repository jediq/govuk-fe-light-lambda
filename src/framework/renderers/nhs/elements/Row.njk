{% macro element(elementData) %}

<div class="nhsuk-grid-row">
{% for eachElement in elementData.elements %}

  <div class="nhsuk-grid-column-{{elementData.fractionWords[elementData.columnWidths[loop.index0]]}}">
{% from "elements/" + eachElement.type + ".njk" import element %}
{{ element(eachElement) }}

  </div>
{% endfor %}

</div>

{% endmacro %}
